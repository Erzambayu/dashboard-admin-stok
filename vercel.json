{
  "functions": {
    "pages/api/**/*.js": {
      "maxDuration": 10
    }
  },
  "regions": ["sin1"],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/pages/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=60, stale-while-revalidate"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "env": {
    "JWT_SECRET": "22d00af652fb289d766518e98846bc73ed88fac30838af645ad2a92a86c107bc"
  }
}